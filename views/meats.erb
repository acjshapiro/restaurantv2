<h1 class="center">Meats</h1>
<div class="row">
  <% @meats.each do |meat| %>
    <div class="col s12 m3">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"><%= meat.name %></span>
          <p><%= meat.description %></p>
        </div>
        <div class="card-action">
          <a class="btn" href="/apps/<%= meat.id %>/edit">Edit</a>
          <a href="#">Delete</a>
        </div>
      </div>
    </div>
  <% end %>
</div>
